<!DOCTYPE html>
<html>
<head>
  <%- include ("../partials/header.ejs") %>
</head>

<body>
  <%- include ("../partials/nav.ejs") %>
  <div class="container">
      <% results.forEach(function(r) { %)>
        <svg>
            <rect width="<%= r.width %>" height="<%= r.height %>" style="fill:blue"></rect>
        </svg> 
      <% }); %>
<table>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Width</th>
      <th>Height</th>
      <th>Color</th>
    </tr>
    <% results.forEach(function(r) { %>
      <td><%= r.id %></td>
      <td><a href=""><%= r.name %></a></td>
      <td><%= r.width %></td>
      <td><%= r.height %></td>
      <td><%= r.color %></td>
    </tr>
    <% }); %>
  </table>
  <button><a href="/database">Return</a></button>
  </div>
</body>
</html>
